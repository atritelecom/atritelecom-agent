{
  "name": "atritelecom-agent",
  "version": "1.0.0",
  "description": "Agente Atritelecom para monitoramento remoto",
  "main": "agent.js",
  "scripts": {
    "start": "node agent.js",
    "build": "pkg agent.js --targets node18-win-x64 --output dist/AtritelecomAgent.exe",
    "build-all": "pkg agent.js --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --output dist/AtritelecomAgent"
  },
  "bin": "agent.js",
  "pkg": {
    "assets": [],
    "targets": ["node18-win-x64"],
    "outputPath": "dist"
  },
  "dependencies": {
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "author": "Atritelecom",
  "license": "MIT"
}
